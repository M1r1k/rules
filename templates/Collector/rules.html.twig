{% block toolbar %}
  {% set icon %}
  <a href="{{ url("webprofiler.profiler", {profile: token}, {fragment: 'rules'}) }}" title="{{ 'Rules'|t }}">
    <img width="28" height="28" alt="{{ 'Rules'|t }}" style="width: 28px"
         src="data:image/png;base64,"/>
    <span class="sf-toolbar-info-piece-additional sf-toolbar-status">{{ collector.getlogscount }}</span>
  </a>
  {% endset %}
  {% set text %}

  <div class="sf-toolbar-info-piece">
    <b>{{ 'Error log entries'|t }}</b>
    <span>{{ collector.geterrorlogscount }}</span>
  </div>

  <div class="sf-toolbar-info-piece">
    <b>{{ 'Notice log entries'|t }}</b>
    <span>{{ collector.getnoticelogscount }}</span>
  </div>

  <div class="sf-toolbar-info-piece">
    <b>{{ 'Info log entries'|t }}</b>
    <span>{{ collector.getinfologscount }}</span>
  </div>

  {% endset %}

  <div class="sf-toolbar-block">
    <div class="sf-toolbar-icon">{{ icon|default('') }}</div>
    <div class="sf-toolbar-info">{{ text|default('') }}</div>
  </div>
{% endblock %}

{% block panel %}
  <div class="title">
    <h2>{{ 'Rules'|t }}</h2>
  </div>

  <div class="content">
    {{ content }}
  </div>
{% endblock %}
